Cell[BoxData[{
 RowBox[{"Remove", "[", "marginalStatements", "]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"marginalStatements", "::", "usage"}], "=", 
   "\"\<marginalStatements[\!\(\*StyleBox[\"stat\",FontSlant->\"Italic\"]\),\!\
\(\*StyleBox[\"nemo\",FontSlant->\"Italic\"]\)] returns a list of joint \
statements which comprise the marginal sum of \
\!\(\*StyleBox[\"stat\",FontSlant->\"Italic\"]\).\n  The Mathematica Symbol \
\!\(\*StyleBox[\"nemo\",FontSlant->\"Italic\"]\) is the Point Nemo for the \
state space.\n  \
marginalStatements[\!\(\*StyleBox[\"stat\",FontSlant->\"Italic\"]\),\!\(\*\
StyleBox[\"nemo\",FontSlant->\"Italic\"]\)] returns an empty List {} when the \
joint statement \!\(\*StyleBox[\"stat\",FontSlant->\"Italic\"]\) is not in \
the state space \!\(\*StyleBox[\"nemo\",FontSlant->\"Italic\"]\).\n\
marginalStatements[] calls stateSpace[] and statementVector[].\>\""}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"marginalStatements", "::", "badarg"}], "=", 
   "\"\<bad argument to marginalStatements.\>\""}], ";"}]}], "Input",
 InitializationCell->True,
